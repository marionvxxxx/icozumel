{
  "name": "cozumel-marketplace",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "cd apps/admin && npm run build",
    "dev": "pnpm --filter @cozumel/admin dev",
    "lint": "cd apps/admin && npm run lint",
    "typecheck": "cd apps/admin && npm run typecheck",
    "clean": "rm -rf node_modules apps/web/node_modules packages/*/node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "cd packages/database && npm run db:generate",
    "db:push": "cd packages/database && npm run db:push",
    "db:seed": "cd packages/database && npm run db:seed"
  },
  "devDependencies": {
    "@turbo/gen": "^1.11.2",
    "prettier": "^3.1.1",
    "turbo": "^2.5.5",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.12.1",
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=8.0.0"
  }
}